 @Nullable private static byte[] getSalt(){   byte[]=null;   if (IConfiguration == null || ((byte[]=IConfiguration.getObfuscationSalt()) == null)) {     Log.w(String,"Can't (un)obfuscate purchases without byte[]");   }   return byte[]; } 
