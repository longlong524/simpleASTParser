public void testGetBoolOrNumberAsBoolShortTrue(){   assertEquals(Boolean.TRUE,IntentUtil.getBoolOrNumberAsBoolExtra(new Intent().putExtra("SHORT_VALUE",new Short("1")),false,"SHORT_VALUE")); } 
