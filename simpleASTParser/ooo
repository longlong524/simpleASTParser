public void testGetBoolOrNumberAsBoolByteTrue(){   assertEquals(Boolean.TRUE,IntentUtil.getBoolOrNumberAsBoolExtra(new Intent().putExtra("BYTE_VALUE",new Byte("1")),false,"BYTE_VALUE")); } 
