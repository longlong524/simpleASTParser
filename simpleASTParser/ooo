private MatrixCursor buildMatrixCursor(int){   final MatrixCursor=new MatrixCursor(new String[]{String,String});   for (int i=0; i < int; i++) {     MatrixCursor.addRow(new Object[]{i,18L});   }   return MatrixCursor; } 
